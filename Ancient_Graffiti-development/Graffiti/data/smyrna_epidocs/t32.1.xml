                    <lb n="1"/>ὑπ’ ὀψὲ ὄρη Πάρις
                    <lb n="2"/>ΠΕ<gap reason="illegible" quantity="2" unit="character"/>Ε<gap reason="illegible" quantity="1" unit="character"/>ΟΡΕΦ<gap reason="lost" extent="unknown" unit="character"/>
